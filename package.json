{
  "name": "syncq",
  "version": "1.0.0",
  "description": "Synchronous queue for asynchronous Javascript Fetch() - when you need sequential execution but still want all that async promise goodness",
  "main": "syncFetch.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "jest",
    "test:with-coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "build:umd-dev": "webpack --output-filename syncQ.js --mode development",
    "build:umd-production": "webpack --output-filename syncQ.min.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foolishsailor/syncFetch.git"
  },
  "keywords": [
    "sync",
    "queue",
    "manager",
    "fetch",
    "async"
  ],
  "author": "foolishsailor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foolishsailor/syncFetch/issues"
  },
  "homepage": "https://github.com/foolishsailor/syncFetch#readme",
  "devDependencies": {
    "jest": "^26.1.0"
  },
  "dependencies": {
    "save-dev": "0.0.1-security",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
